        {
          "expr": "aws_rds_write_iops_average",
          "legendFormat": "Write IOPS - {{dimension_DBInstanceIdentifier}}",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "iops",
          "thresholds": {
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 1000
              },
              {
                "color": "red",
                "value": 3000
              }
            ]
          }
        }
      }
    },
